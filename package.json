{
  "name": "sbs-discord-bridge",
  "version": "1.0.0",
  "description": "a bridge between sbs rooms and discord channels",
  "main": "build/index.js",
  "bin": "build/index.js",
  "scripts": {
    "start": "tsc --build --verbose && node .",
    "inspect": "tsc --build --verbose && node --inspect .",
