    "beep": "cat beep"
  },
  "author": "auceps",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^16.3.3",
    "axios": "^0.21.1",
    "discord.js": "^13.0.0-dev.63ce065.1626653031",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "^4.3.5"
  }
}
